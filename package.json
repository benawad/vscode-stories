{
  "name": "stories",
  "displayName": "Stories",
  "icon": "media/logo2.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ide-stories/vscode-stories"
  },
  "description": "Stories for VSCode",
  "publisher": "bar9",
  "version": "2.23.0",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "Stories",
        "properties": {
          "stories.ffmpeg-binary": {
            "description": "FFmpeg Binary Location",
            "type": "string"
          },
          "stories.recording-location": {
            "description": "Recordings Location",
            "type": "string"
          },
          "stories.story-type": {
            "description": "Story Type",
            "type": "string",
            "default": "none",
            "enum": [
              "text",
              "gif",
              "none"
            ],
            "enumDescriptions": [
              "Recording defaults to text story",
              "Recording defaults to gif story",
              "Select story type at runtime with a quick pick selection"
            ]
          }
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "stories.refresh",
          "when": "view == storiesPanel || view == stories-full",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "stories.refresh",
        "title": "Refresh",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      },
      {
        "title": "Authenticate",
        "category": "Stories",
        "command": "stories.authenticate"
      },
      {
        "title": "Set Flair",
        "category": "Stories",
        "command": "stories.setFlair"
      },
      {
        "title": "Start Text Recording",
        "category": "Stories",
        "command": "stories.startTextRecording"
      },
      {
        "title": "Stop Text Recording",
        "category": "Stories",
        "command": "stories.stopTextRecording"
      },
      {
        "title": "Start Gif Recording",
        "category": "Stories",
        "command": "stories.startGifRecording"
      },
      {
        "title": "Stop Gif Recording",
        "category": "Stories",
        "command": "stories.stopGifRecording"
      },
      {
        "title": "Set Story Type Config",
        "category": "Stories",
        "command": "stories.setStoryTypeConfig"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "stories-icon",
          "title": "Stories",
          "icon": "media/circle-s-outline.svg"
        }
      ]
    },
    "views": {
      "stories-icon": [
        {
          "type": "webview",
          "id": "stories-full",
          "name": "Stories",
          "icon": "media/circle-s-outline.svg",
          "contextualTitle": "Stories"
        }
      ],
      "explorer": [
        {
          "type": "webview",
          "id": "storiesPanel",
          "name": "Stories"
        }
      ]
    },
    "keybindings": [
      {
        "command": "stories.refresh",
        "key": "",
        "mac": ""
      },
      {
        "command": "stories.setFlair",
        "key": "",
        "mac": ""
      },
      {
        "command": "stories.startTextRecording",
        "key": "",
        "mac": ""
      },
      {
        "command": "stories.stopTextRecording",
        "key": "",
        "mac": ""
      },
      {
        "command": "stories.startGifRecording",
        "key": "",
        "mac": ""
      },
      {
        "command": "stories.stopGifRecording",
        "key": "",
        "mac": ""
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "rollup -c && webpack --mode production && yarn run copyArciisineFile",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "copyArciisineFile": "copyfiles -u 4 ./node_modules/@arcsine/win-info/bin/win-info-win32.exe ./node_modules/@arcsine/win-info/bin/win-info-x11.js ./dist/bin",
    "dev": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\" \"yarn run copyArciisineFile\"",
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "start": "sirv public",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js",
    "test-svelte": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/btoa": "^1.2.3",
    "@types/express": "^4.17.8",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.15",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/node-fetch": "^2.5.7",
    "@types/polka": "^0.5.1",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mocha": "^8.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-loader": "^8.0.9",
    "tslib": "^2.0.0",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@arcsine/screen-recorder": "^0.1.6",
    "@types/uuid": "^8.3.0",
    "btoa": "^1.2.1",
    "copyfiles": "^2.4.1",
    "express": "^4.17.1",
    "fastify": "^3.7.0",
    "form-data": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "polka": "^0.5.2",
    "unique-names-generator": "^4.3.1",
    "uuid": "^8.3.2"
  }
}
